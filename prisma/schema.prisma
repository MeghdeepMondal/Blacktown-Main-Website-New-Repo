generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model adminrequests {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  address        String
  contactDetails String
  createdAt      DateTime @db.Date
  description    String
  email          String   @unique
  lat            Float
  lng            Float
  logo           Bytes?
  name           String
  password       String
  status         String
  updatedAt      DateTime @db.Date
}

model admins {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  address        String?
  contactDetails String?
  createdAt      DateTime? @db.Date
  description    String?
  email          String    @unique
  lat            Float?
  lng            Float?
  logo           Bytes?
  name           String
  password       String
  updatedAt      DateTime? @db.Date
}

model events {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @db.Date
  date        DateTime @db.Date
  description String
  frequency   String
  lat         Float
  lng         Float
  location    String
  name        String
  updatedAt   DateTime @db.Date
}

model members {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model sessions {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  jwt     String
  user_id String @unique(map: "user_id_1")
}
